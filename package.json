{
  "name": "bootstrap",
  "version": "1.0.0",
  "description": "Jeykll project to maintain all the landing pages and assets. Use Ruby 2.3.0.",
  "main": "gulpfile.js",
  "scripts": {
    "setup": "npm install -g gulp-cli && npm install && gem install bundler && bundle install",
    "dev": "gulp server",
    "force": "gulp javascripts --force && gulp stylesheets --force && jekyll build",
    "upgrade": "npm install -g npm-check-updates && npm-check-updates -u && npm install && gem install bundler && bundle update",
    "crunch": "gulp optimize_images"
  },
