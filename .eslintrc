{
  "parser": "babel-eslint",
  "rules": {
    "brace-style": 2,
    "camelcase": [2, { "properties": "never" }], // TQ API uses snake casing
    "complexity": [0, 11],
    "curly": [2, "multi-line"],
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 0,
    "max-params": [0, 3],
    "max-statements": [0, 10],
    "new-cap": 0,
    "new-parens": 2,
    "no-alert": 2,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-console": 2,
    "no-dangle": 0,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-empty": 2,
    "no-eval": 1,
    "no-ex-assign": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 0,
    "no-implied-eval": 2,
    "no-label-var": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-array": 0,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-plusplus": 0,
    "no-return-assign": 2,
    "no-self-compare": 0,
    "no-ternary": 0,
    "no-undef-init": 2,
    "no-unreachable": 2,
    "no-with": 2,
    "quote-props": 0,
    "quotes": [2, "single"],
    "radix": 0,
    "regex-spaces": 0,
    "semi": 2,
    "smarter-eqeqeq": 0,
    "use-isnan": 2,
    "wrap-iife": 1
  }
}